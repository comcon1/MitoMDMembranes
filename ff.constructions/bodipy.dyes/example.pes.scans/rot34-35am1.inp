 $CONTRL
  SCFTYP=RHF EXETYP=RUN RUNTYP=rsurface
! B3LYP in US means B3LYP5 in firefly
!  DFTTYP=B3LYP5
  ICHARG=0 MULT=1
! ITOL=30 ICUT=11 INTTYP=HONDO MAXIT=200
! d5=.t.
  NZVAR=1
 $END

 $statpt opttol=1e-5 nstep=2000
! frozen internal coordinate
 method=gdiis nstep=2000 ifreez(1)=1
 $end

 $zmat
  dlc=.t. auto=.t.
  ifzmat(1)=3,33,34,35,36
!  irzmat(1)=3,34,33,32,27
  scan=.t.
  dlctol=1d-7 orttol=1d-10 ifdmod=2
 $end

 $surf disp1=10 ndisp1=37 reuse=.t. $end

!$d5 d5=.f. $end

 $SYSTEM TIMLIM=2000 MWORDS=100 $END

 $GUESS GUESS=HUCKEL $END

 $basis
! name from lib-file
!   gbasis=N31 ngauss=6 npfunc=1 ndfunc=1 nffunc=1
   gbasis=am1
! diffsp=.t.
 $end

 $SCF
  DIIS=.t. DIRSCF=.t.
 $END

